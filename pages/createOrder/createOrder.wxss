  font-size:26rpx;
  line-height:42rpx;
  color: #fff;
  text-align: center;
}
.coupon_btn[data-can_get='0']{
  background: #D8D8D8;
}
.coupon_shot_note{
  font-size: 20rpx;
  color: #8E8E8E;
  line-height: 1;
  padding: 32rpx 30rpx 0 30rpx;
}
.coupon_shot_note text{
  display: inline-block;
  width: 200rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.coupon_shot_note_ico{
  width: 28rpx;
  height: 28rpx;
  background: none;
  float: right;
  transition: all .2s;
  transform: rotateZ(0deg);
}

.coupon_note{
  display: none;
  line-height: 44rpx;
  font-size: 20rpx;
  color: #8E8E8E;
  background: #fff;
  padding: 42rpx 30rpx 8rpx;
  margin-top: -26rpx;
  border-radius:  0 0 12rpx 12rpx;
}

.open_note .coupon_shot_note_ico{
  transform: rotateZ(180deg);
}
.open_note .coupon_note{
  display: block;
}
