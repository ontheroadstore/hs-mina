.main{
  background: #F5F5F5;
}
.main_content{
  min-height: 105vh;
}
.address{
  background: #FFF;
  position: relative;
  padding: 30rpx 88rpx 30rpx 100rpx;
  height: auto;
  width: auto;
  border-top: 2rpx solid #E8E8E8;
}
.address .address_icon{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  left: 26rpx;
  top: 50%;
  transform: translateY(-50%);
  background: none;
}
.address .user_tel{
  font-size: 26rpx;
  color: #202123;
  position: absolute;
  top: 30rpx;
  right: 88rpx;
}
.address .user_name{
  font-size: 26rpx;
  font-weight: 500;
  margin-bottom: 20rpx;
}
.address .address_details{
  font-size: 26rpx;
  color: #2D2F3C;
  line-height: 32rpx;
  font-weight: 300;
}
.address .gt_icon{
  width: 16rpx;
  height: 26rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  background: none;
}
.address .no_address{
  font-size: 32rpx;
  font-weight: 500;
  height: 100rpx;
  line-height: 100rpx;
}


.orders,
.order{
  padding-bottom: 270rpx;
}
.orders_iphoneX{
  padding-bottom: 190rpx;
}
.orders .user_item,
.order .user_item{
  background: #FFF;
  margin-top: 16rpx;
  padding: 0 30rpx;
}
.orders .user_info,
.order .user_info{
  padding: 30rpx 0;
  height: 40rpx;
  line-height: 40rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.orders .user_info image,
.order .user_info image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.orders .user_info view,
.order .user_info view{
  font-size: 26rpx;
}
.orders .order_item,
.order .order_item{
  position: relative;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #E8E8E8;
}
.orders .order_item .image,
.order .order_item .image{
  border-radius: 12rpx;
  width: 286rpx;
  height: 146rpx;
  margin-right: 30rpx;
}
.orders .order_item .good_title,
.order .order_item .good_title{
  font-size: 26rpx;
  text-overflow: ellipsis;
  overflow: hidden; 
  white-space: nowrap;
  width: 360rpx;
  margin-bottom: 40rpx;
}
.orders .order_item .good_type,
.order .order_item .good_type{
  text-overflow: ellipsis;
  overflow: hidden; 
  white-space: nowrap;
  width: 360rpx;
  color: #8E8E8E;
  font-size: 24rpx;
}
.orders .order_item .good_price,
.order .order_item .good_price{
  color: #AE2121;
  font-size: 26rpx;
  position: absolute;
  left: 316rpx;
  top: 140rpx;
  line-height: 1em;
}
.orders .order_item .count_num,
.order .order_item .count_num{
  font-size: 26rpx;
  position: absolute;
  right: 0;
  top: 140rpx;
  line-height: 1em;
  color: #8E8E8E;
}

.order_item .count_num .add_icon,
.order_item .count_num .sub_icon{
  width: 35rpx;
  height: 35rpx;
  border: 1rpx solid #E8E8E8;
  background: none;
  padding: 6rpx;
  box-sizing: border-box;
}
.order_item .count_num .good_num{
  display: block;
  width: 64rpx;
  text-align: center;
  height: 35rpx;
  line-height: 35rpx;
}


.postage_num,
.desc{
  border-bottom: 2rpx solid #E8E8E8;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
}
.postage_num .label,
.desc .label{
  font-weight: 600;
}
.desc{
  border: none;
}
