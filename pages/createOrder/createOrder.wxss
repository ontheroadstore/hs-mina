.quanyi_btn{
  font-size: 21rpx;
  color: #8E8E8E;
}
.quanyi_btn view{
  height: 40rpx;
 line-height: 54rpx;
}
.quanyi_price{
  color: #AE2121;
}
.quanyi_price text{
  color: #8E8E8E;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.quanyi_icon{
  float: left;
  background: #fff;
  width: 27rpx;
  height: 17rpx;
  margin-top: 20rpx;
  margin-right: 6rpx;
}
.quanyi_icon2{
  display: inline-block;
  background: #fff;
  width: 21rpx;
  height: 21rpx;
  margin-left: 10rpx;

}
.desc input{
  height: 88rpx;
  line-height: 88rpx;
  width: 560rpx;
  margin-left: 20rpx;
}





.buy{
  position: fixed;
  bottom: 0;
  background: #FFF;
  border-top: 2rpx solid #E8E8E8;
  height: 96rpx;
  width: 100vw;
  z-index: 999;
}
.buy .buy_btn{
  position: relative;
  width: 256rpx;
  height: 98rpx;
  top: -2rpx;
  color: #FFF;
  text-align: center;
  line-height: 98rpx;
  font-size: 32rpx;
  background: #AE2121;
}
.buy .postage{
  color: #8E8E8E;
  font-size: 24rpx;
  line-height: 96rpx;
  margin: 0 30rpx 0 20rpx;
}
.buy .tatol{
  font-size: 24rpx;
  line-height: 96rpx;
}
.buy .tatol text{
  color: #AE2121;
  font-size: 28rpx;
}
.buy_iphoneX{
  padding-bottom: 68rpx;
}
.use-coupon{
  width: 690rpx;
  height: 80rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  position: absolute;
  top: -80rpx;
  left: 0;
  line-height: 80rpx;
  background: #fff;
  border-bottom: 1px solid #ccc;
  border-top:2rpx solid #E8E8E8;

}
.full_reduce_total{
  width: 690rpx;
  height: 80rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  position: absolute;
  top: -160rpx;
  left: 0;
  line-height: 80rpx;
  background: #fff;
  border-bottom: 1px solid #ccc;
  border-top:2rpx solid #E8E8E8;                                                                         
}
.use-coupon .gt_icon,.full_reduce_total .gt_icon{
  width: 16rpx;
  height: 26rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  background: none;
}
.use-coupon .float_right,.full_reduce_total .float_right{
  color: #ccc;
  padding-right:60rpx;
}
/* 优惠券弹窗 */
.coupon_popup_mask{
  position: fixed;
  z-index: 2000;
  left: 0;
  right: 0;
  top:0;
  bottom: 0;
  background: rgba(0,0,0,.3);

}
.coupon_popup_wrap{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 832rpx;
  background: #F5F5F5;
  border-radius:24rpx 24rpx 0 0;
  overflow: hidden;
}
.coupon_popup_title{
  height:112rpx;
  font-size:32rpx;
  color:#000;
  line-height:112rpx;
  text-align: center;
  background: #FFF;
}
.coupon_popup_ul{
  height: 560rpx;
}
.coupon_popup_li{
  height: 144rpx;
  width: 690rpx;
  padding: 20rpx 0;
  margin: 0 auto 30rpx;
  background: #fff;
  border-radius: 12rpx;
}
.coupon_popup_li:last-child{
  margin-bottom: 0;
}
.coupon_popup_left{
  float: left;
  width: 222rpx;
  height: 100%;
  text-align: center;
  border-right: 1rpx dotted #979797;
}
.coupon_popup_price{
  color: #AE2121;
  font-size: 52rpx;
  line-height: 1;
  padding: 26rpx 0 22rpx 0;
}
.coupon_popup_tip{
  font-size:20rpx;
  color:#202123;
  line-height:1;
}
.coupon_popup_right{
  float: left;
  width: 464rpx;
  height: 100%;
  position: relative;
}
.coupon_popup_name{
  padding: 26rpx 0 48rpx 30rpx;
  width: 238rpx;
  font-size: 28rpx;
  color: #202123;
  line-height: 1;
  height: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.coupon_popup_time{
  padding: 0 0 0 30rpx;
  font-size: 20rpx;
  color: #8E8E8E;
  line-height: 1;
}
.coupon_popup_btn{
  position: absolute;
  right: 30rpx;
  top: 52rpx;
  width:68px;
  height:42rpx;
  background:#AE2121;
  border-radius:22rpx;
  font-size:26rpx;
  line-height:42rpx;
  color: #fff;
  text-align: center;
}
.coupon_popup_btn[data-can_get='0']{
  background: #D8D8D8;
}
.coupon_popup_ok{
  width: 678rpx;
  height: 80rpx;
  background:#AE2121;
  border-radius: 12rpx;
  margin: 40rpx auto 0;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
}
.sub-main{
  height: 560rpx;
  padding:80rpx 0 0;
  box-sizing: border-box;
}
.tag{
  width:100%;
  height:80rpx;
  background:#fff;
  display: flex;
  align-items: center;
  justify-content: space-around;
  position: absolute;
  left:0;
  top:112rpx;
  z-index:999;
}
.tag text{
  font-size:28rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(32,33,35,1);
  line-height:28rpx;
}
.tag text.active{
  font-weight:600;
  color:rgba(174,33,33,1);
  position:relative
}
.tag text.active::after{
  content:'';
  position:absolute;
  top:38rpx;
  left:50%;
  margin-left:-20rpx;
  width:40rpx;
  height:6rpx;
  background:rgba(174,33,33,1);
  border-radius:8rpx;
}
