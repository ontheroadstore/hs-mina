  position: relative;
}
.coupon_time::after{
  content: '';
  display: block;
  width: 400rpx;
  border-bottom: 4rpx dotted #979797;
  position: absolute;
  left: 30rpx;
  bottom: -16rpx;
}
.coupon_btn{
  position: absolute;
  right: 30rpx;
  top: 52rpx;
  width:68px;
  height:42rpx;
  background:#AE2121;
