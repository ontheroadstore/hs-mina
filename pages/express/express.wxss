.order_info{
  padding: 0 30rpx;
  font-size: 28rpx;
  font-weight: 300;
}
.order_info text{
  font-size: 26rpx;
  font-size: 100;
}
.express_status{
  padding: 30rpx;
  font-weight: 600;
}
.express_details{
  margin: 50rpx 30rpx 0;
  border-top: 2rpx solid #E8E8E8;
}
.express_details .title{
  padding: 30rpx 0;
  font-size: 28rpx;
  font-weight: 600;
}
.express_content{
  padding: 22rpx 0 40rpx;
}
.express_item{
  position: relative;
  height: 68rpx;
  margin-bottom: 80rpx;
}
.express_item:after{
  content: "";
  display: block;
  position: absolute;
  width: 2rpx;
  height: 140rpx;
  background: #B2B2B2;
  top: 34rpx;
  left: 173rpx;
}
.clearBorder:after{
  content: "";
  width: 0;
  height: 0;
}
.express_item .date{
  width: 186rpx;
  line-height: 34rpx;
  color: #888888;
  padding-top: 7rpx;
}
.express_item .date .day{
  font-size: 26rpx;
  font-weight: 500;
}
.express_item .date .time{
  font-size: 24rpx;
  font-weight: 300;
  line-height: 1em;
}
.express_item .up_icon{
  position: absolute;
  top: 21rpx;
  left: 161rpx;
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  z-index: 1;
}
.icon_baoguan{
  position: absolute;
  top: -8rpx;
  left: 146rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  z-index: 1;
}
.express_item .details{
  float: right;
  width: 450rpx;
  height: 68rpx;
  overflow: hidden;
  line-height: 34rpx;
  font-size: 26rpx;
  color: #888888;
}
.express_item .date.active,
.express_item .details.active{
  color: #151515;
}

.random_goods{
  border-top: 16rpx solid #F5F5F5;
  padding: 0 30rpx 100rpx 30rpx;
}
.random_goods .model_title{
  padding: 30rpx 0;
  font-weight: 600;
  font-size: 32rpx;
}
.random_goods .good_item{
  padding-bottom: 40rpx;
  width: 690rpx;
}
.random_goods .cover{
  width: 336rpx;
  height: 172rpx;
  border-radius: 12rpx;
  margin-right: 30rpx;
}
.random_goods .article_title{
  width: 324rpx;
  overflow: hidden;
  height: 72rpx;
  line-height: 36rpx;
  font-size: 26rpx;
}
.random_goods .user_avatar{
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  margin: 18rpx 12rpx 16rpx 0;
}
.random_goods .user_name{
  font-size: 24rpx;
  line-height: 32rpx;
  color: #8E8E8E;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 280rpx;
  margin: 18rpx 0 16rpx 0;
}
.random_goods .price{
  font-size: 32rpx;
  line-height: 1em;
}
.no_data{
  font-size: 32rpx;
  color: #EC3B2A;
}