.frontcover{
  width: 100%;
  height: 382rpx;
}
.good_title{
  padding: 30rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  font-weight: 700;
}
.special{
  padding: 0 30rpx 30rpx 30rpx;
  line-height: 1em;
  font-size: 26rpx;
  color: #202123;
}
.share_content{
  position: relative;
  height: 86rpx;
  border-bottom: 16rpx solid #F5F5F5;
}
.good_price{
  font-size: 40rpx;
  position: absolute;
  left: 30rpx;
}
.good_price .new_price{
  color: #AE2121;
}
.good_price .now_price{
  font-size: 32rpx;
  color: #888;
  position: relative;
  margin-left: 28rpx;
  height: 1em;
  font-weight: 300;
}
.good_price .now_price:after{
  content: "";
  position: absolute;
  width: 130%;
  height: 2rpx;
  top: 50%;
  left: -5%;
  transform: translateY(-50%);
  background: #888;
}
.share{
  width: 48rpx;
  height: 48rpx;
  background: #FFF;
  position: absolute;
  right: 30rpx;
  top: 0;
}
.share_btn{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 0;
  right: 30rpx;
   opacity: 0; 
}
.user_introduce{
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #E8E8E8;
}
.user_introduce image{
  width: 66rpx;
  height: 66rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.user_introduce text{
  font-size: 26rpx;
  height: 66rpx;
  line-height: 34rpx;
  width: 578rpx;
}
.style{
  position: relative;
  margin: 0 30rpx;
  padding-left: 94rpx;
  font-size: 28rpx;
  padding: 30rpx 0 30rpx 114rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.style .label{
  color: #888;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}
.style text{
  line-height: 32rpx;
  width: 500rpx;
  display: block;
}
.style .more{
  width: 16rpx;
  height: 26rpx;
  background: none;
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
}
.presell{
  position: relative;
  margin: 0 30rpx;
  padding-left: 94rpx;
  font-size: 28rpx;
  padding: 30rpx 0 30rpx 114rpx;
  border-bottom: 2rpx solid #E8E8E8;
}
.presell .label{
  color: #888;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}
.presell text{
  line-height: 32rpx;
  width: 500rpx;
  display: block;
}





.desc{
  padding: 30rpx;
  border-bottom: 16rpx solid #F5F5F5;
  margin-bottom: 30rpx;
}
.content{
  padding: 30rpx;
}
.desc .title,
.content .title{
  font-weight: 700;
  font-size: 32rpx;
  line-height: 32rpx;
  margin-bottom: 30rpx;
  display: block;
}
.desc .text_details,
.content .text_details{
  font-size: 28rpx;
  line-height: 32rpx;
  font-weight: 200;
  font-family:  normal;
}
.images{
  padding: 30rpx 0 30rpx 30rpx;
}
.images image{
  width: 216rpx;
  height: 216rpx;
  float: left;
  margin-bottom: 20rpx;
  margin-right: 21rpx;
}
